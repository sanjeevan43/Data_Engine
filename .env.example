# ============================================
# SmartImport - Environment Variables
# ============================================
# Copy this file to .env and fill in your credentials
# Only configure the provider(s) you plan to use

# ============================================
# FIREBASE
# ============================================
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# ============================================
# SUPABASE
# ============================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ============================================
# MONGODB (Data API)
# ============================================
VITE_MONGO_API_URL=https://data.mongodb-api.com/app/your-app-id/endpoint/data/v1
VITE_MONGO_API_KEY=your_mongodb_api_key_here
VITE_MONGO_DATA_SOURCE=Cluster0
VITE_MONGO_DATABASE_NAME=your_database_name

# ============================================
# AWS AMPLIFY
# ============================================
VITE_AMPLIFY_API_URL=https://xxxxx.appsync-api.us-east-1.amazonaws.com/graphql
VITE_AMPLIFY_API_KEY=da2-xxxxxxxxxxxxx
VITE_AMPLIFY_REGION=us-east-1

# ============================================
# APPWRITE
# ============================================
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your_project_id
VITE_APPWRITE_DATABASE_ID=your_database_id

# ============================================
# POCKETBASE
# ============================================
VITE_POCKETBASE_URL=http://127.0.0.1:8090
# or for production: https://your-pocketbase.com

# ============================================
# GENERAL SETTINGS
# ============================================
VITE_DEFAULT_PROVIDER=Firebase
VITE_DEFAULT_COLLECTION=csv_imports